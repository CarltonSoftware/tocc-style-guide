{
  "name": "tocc-style-guide",
  "version": "1.0.0",
  "description": "Style guide for the Original Cottage Company",
  "main": "index.js",
  "author": "Alex Wyett",
  "scripts": {
    "copycss": "rm -Rf public/css && cp -Rf node_modules/toccstyles/dist public/css",
    "start": "yarn run copycss && PORT=7777 react-scripts start",
    "build": "react-scripts build",
    "predeploy": "react-scripts build && cp CNAME build/CNAME",
    "deploy": "gh-pages -d build"
  },
  "homepage": "https://styleguide.originalcottages.co.uk",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.462.0",
    "bulma": "^0.7.4",
    "details-polyfill": "^1.1.0",
    "dotenv": "^8.0.0",
    "fs-extra": "^7.0.1",
    "gh-pages": "^2.0.1",
    "node-sass": "^4.11.0",
    "object-path": "^0.11.4",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.1",
    "react-dom": "^16.8.6",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "tabsutils": "github:carltonsoftware/tabsutils"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
